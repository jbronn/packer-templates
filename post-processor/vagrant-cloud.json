{
    "post-processors": [
        [
            {
                "type": "vagrant",
                "compression_level": "9",
                "output": "output/{{user `vm_name`}}_{{.Provider}}.box",
                "vagrantfile_template": "{{user `vagrantfile_template`}}"
            },
            {
                "type": "vagrant-cloud",
                "access_token": "{{user `access_token`}}",
                "box_tag": "{{user `box_tag`}}",
                "version": "{{user `box_version`}}"
            }
        ]
    ],
    "variables": {
        "access_token": null,
        "box_tag": null,
        "box_version": "{{isotime \"20060102\"}}.0.0",
        "disk_size": "40960"
    }
}
