{
    "post-processors": [
        {
            "type": "vagrant",
            "compression_level": "9",
            "output": "{{user `output_dir`}}/{{user `vm_name`}}_{{.Provider}}.box",
            "vagrantfile_template": "{{user `vagrantfile_template`}}"
        }
    ],
    "variables": {
        "disk_size": "40960",
        "output_dir": "output"
    }
}
