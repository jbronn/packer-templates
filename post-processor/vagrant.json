            "compression_level": "9",
            "output": "output/{{user `vm_name`}}_{{.Provider}}.box",
            "vagrantfile_template": "{{user `vagrantfile_template`}}"
        }
    ],
    "variables": {
        "disk_size": "40960"
    }
}
