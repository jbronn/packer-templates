{
    "builders": [
        {
            "type": "virtualbox-iso",
            "boot_wait": "{{user `boot_wait`}}",
            "disk_size": "{{user `disk_size`}}",
            "format": "{{user `format`}}",
            "guest_additions_mode": "{{user `guest_additions_mode`}}",
            "guest_additions_path": "{{user `guest_additions_path`}}",
            "guest_os_type": "{{user `guest_os_type`}}",
            "hard_drive_interface": "{{user `hard_drive_interface`}}",
            "headless": "{{user `headless`}}",
            "http_directory": "{{user `http_directory`}}",
            "iso_url": "{{user `iso_url`}}",
            "iso_checksum": "{{user `iso_checksum`}}",
            "iso_interface": "{{user `iso_interface`}}",
            "output_directory": "{{user `build_dir`}}/{{build_name}}",
            "post_shutdown_delay": "{{user `post_shutdown_delay`}}",
            "shutdown_command": "{{user `shutdown_command`}}",
            "ssh_username": "root",
            "ssh_password": "{{user `admin_user_password`}}",
            "ssh_wait_timeout": "{{user `ssh_wait_timeout`}}",
            "vboxmanage": [
                ["modifyvm", "{{.Name}}", "--audio", "none"],
                ["modifyvm", "{{.Name}}", "--cpus", "{{user `cpus`}}"],
                ["modifyvm", "{{.Name}}", "--memory", "{{user `memsize`}}"],
                ["modifyvm", "{{.Name}}", "--nictype1", "{{user `nictype`}}"],
                ["modifyvm", "{{.Name}}", "--usb", "off"]
            ],
            "vboxmanage_post": [
                ["modifyvm", "{{.Name}}", "--vrde", "off"],
                ["storagectl", "{{.Name}}", "--name", "IDE Controller", "--remove"]
            ],
            "vm_name": "{{user `vm_name`}}"
        }
    ],
    "variables": {
        "admin_user": "vagrant",
        "admin_user_authorized_keys": "ssh-rsa  vagrant insecure public key",
        "admin_user_password": "vagrant",
        "admin_user_shell": "/bin/bash",
        "boot_wait": "10s",
        "build_dir": "build",
        "cloud_init": "no",
        "cpus": "1",
        "disk_size": "8192",
        "format": "ovf",
        "guest_additions_mode": "upload",
        "guest_additions_path": "VBoxGuestAdditions.iso",
        "guest_keyboard": "us",
        "guest_timezone": "UTC",
        "guest_language": "en_US.UTF-8",
        "guest_os_type": null,
        "hard_drive_interface": "sata",
        "http_directory": "www",
        "headless": "false",
        "iso_checksum": null,
        "iso_interface": "sata",
        "iso_url": null,
        "memsize": "512",
        "nictype": "virtio",
        "post_shutdown_delay": "10s",
        "shutdown_command": null,
        "ssh_root_password": "vagrant",
        "ssh_wait_timeout": "20m",
        "vagrantfile_template": "vagrant/Vagrantfile.default",
        "vm_name": null
    }
 }
